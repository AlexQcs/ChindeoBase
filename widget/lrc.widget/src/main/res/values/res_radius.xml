<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RadiusImageView">
        <attr name="radius_shape" format="enum">
            <enum name="rect" value="0"/>
            <enum name="circle" value="1"/>
            <enum name="oval" value="2"/>
        </attr>
        <attr name="isTouchSelectModeEnabled" format="boolean"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="selectedBorderWidth" format="dimension"/>
        <attr name="borderColor" format="color"/>
        <attr name="selectedBorderColor" format="color"/>
        <attr name="maskColor" format="color"/>
        <attr name="selectedMaskColor" format="color"/>
        <attr name="radius_radius" format="integer"/>
        <attr name="leftTopRadius" format="integer"/>
        <attr name="rightTopRadius" format="integer"/>
        <attr name="leftBottomRadius" format="integer"/>
        <attr name="rightBottomRadius" format="integer"/>
    </declare-styleable>
</resources>